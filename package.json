{
  "name": "sphere-dev-network",
  "version": "1.0.0",
  "description": "Virtual Internet Development Environment",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/app.js",
    "client": "node node_modules/vite/bin/vite.js --config client/vite.config.js",
    "build": "node node_modules/vite/bin/vite.js build --config client/vite.config.js",
    "start": "node server/app.js",
    "render-build": "npm install && node node_modules/vite/bin/vite.js build --config client/vite.config.js",
    "render-start": "node server/app.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "vite": "^4.5.0",
    "@vitejs/plugin-react": "^4.1.1",
    "nodemon": "^3.0.1",
    "terser": "^5.26.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
